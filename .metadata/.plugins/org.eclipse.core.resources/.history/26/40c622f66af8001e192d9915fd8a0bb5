package am;

public class Sub01 {
	
	public static void run() {
		
		Car car1 = new Car(); //새로운 객체를 만들어서 car1에 저장
		
		car1.company = "현대";
		car1.model = "그랜저";
		car1.color = "검정";
		car1.distance = 60000;
		car1.date = "2021-06-01";
		
		car1.print();
		
		Car car2 = new Car(); //새로운 객체를 만들어서 car2에 저장
		
		car2.company = "기아";
		car2.model = "모닝";
		car2.color = "라임";
		car2.distance = 20000;
		car2.date = "2024-12-04";
		
		car2.print();
		
	}

}
