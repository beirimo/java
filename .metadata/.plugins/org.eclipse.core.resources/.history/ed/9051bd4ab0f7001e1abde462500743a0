package am;

public class Sub02 {
	public static void run() {
		System.out.println("클래스란?");
		
		Student s1 = new Student(); //Student(클래스) s1(객체) Student()(생성자)
		
		s1.no = "103";
		s1.name = "라이언";
		s1.juso = "카카오 프렌즈";
		s1.phone = "010-1472-5836";
		s1.print();
		
		System.out.println("------------------------");
		Student s2 = new Student("123", "춘식이"); //생성자2
		s2.juso = "고구마 상자";
		s2.phone = "010-4862-7913";
		s2.print();
	}

}
